const NFTs = [
    {
        name: "GNARLY"
    }
];

function mintNFT(nft) {
    NFTs.push(nft);
    return nft;
}

function listNFTs() {
    for (let i = 0; i < NFTs.length; i++) {
        console.log(NFTs[i]);
    }
}

function getTotalSupply() {
    console.log(`Total Supply: ${NFTs.length}`);
}

// call your functions below this line

// listNFTs()

mintNFT({ name: "DBTK" });
mintNFT({ name: "HUNDREDP" });
mintNFT({ name: "FRESHANDCOOL" });
listNFTs();

getTotalSupply();
